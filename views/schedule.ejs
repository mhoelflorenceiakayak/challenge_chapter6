<!DOCTYPE html>
<html lang="en">
<head>
    <!-- My CSS -->
    <link href="/css/dashboard.css" rel="stylesheet" type="text/css"/>
    <title>Dashboard</title>
</head>
<body>
    
    
    <input type="checkbox" id="nav-toggle">
    <div class="sidebar">
        <div class="sidebar-brand">
          <h1> <span class="fab fa-asymmetrik"> </span> <span>Games.flo.id</span>
          </h1>
        </div>
        
        <div class="sidebar-menu">
          <ul>
            <li>
              <a href="/dashboard" class="active">
                <span class="fas fa-tachometer-alt"></span>
                <span>Users Data</span>
              </a>
            </li>
            <li>
              <a href="/user/biodata">
                <span class="fas fa-users" ></span>
                <span>Users Biodata</span>
              </a>
            </li>
            <li>
              <a href="/match/schedule">
                <span class="fas fa-stream"></span>
              <span>All Schedule</span>
              </a>
            </li>
          </ul>

        </div>
    </div>    

    <div class="main-content">
      <header>
        <h2>
          <label for="nav-toggle">
            <span class="fas fa-bars"></span>
          </label>
          Dashboard
        </h2>
      </header>

      <main>
        <div class="recent-grid">
          <div class="projects">
            <div class="card">
              <div class="card-header">
                <h2>Match Schedule</h2>
              </div>
              <div class="card-body">
                <div class="table-responsive">
                  <table width="100%">
                  <thead>
                    <tr>
                      <td>Username</td>
                      <td>Match Schedule</td> 
                    </tr>
                  </thead>
                    <tbody>
                         <% match.forEach(i=> { %>
                              <td>
                                <%= i.username %>
                              </td>
                              <td>
                                <%= i.match_schedule %>
                              </td>
                            </tr>
                            <%})%>
                  </tbody>

                </table>
                </div>
              </div>

            </div>

          </div>
          
          
        </div>  

      </main>
    </div>


</body>
</html>